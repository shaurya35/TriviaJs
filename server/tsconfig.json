{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": [
      "index.ts",
      "routes/**/*.ts",
      "models/**/*.ts",
      "controllers/**/*.ts",
      "src/**/*"
    ]
  }
  